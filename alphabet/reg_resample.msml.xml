
<msml:operator xmlns:msml="http://sfb125.de/msml"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://sfb125.de/msml"
               name="reg_resample">

    <annotation>
        <entry key="doc">
            RegResample (NiftyReg)
            ===========

            

            NiftyReg module for resampling using input transformation

            :License: BSD
            :Contributor: Marc Modat (UCL)
        </entry>
        <entry key="category" value="Registration.NiftyReg" />
    </annotation>


    <runtime>
        <sh template="reg_resample --inter={interpolation} --pad={paddingValue}  --res={warpedImage} --blank={warpedGrid}  --ref={referenceImageName} --flo={floatingImageName} --trans={inputTransformation} "/>
    </runtime>

    <input>
    
        <arg name="referenceImageName"  physical="Image.NII,Image.NII.GZ,Image.NRRD,Image.PNG" default="required"  >
            <meta key="doc">
                Reference image
                    
                    Reference image filename (also called Target of Fixed)
            </meta>
        </arg>

        <arg name="floatingImageName"  physical="Image.NII,Image.NII.GZ,Image.NRRD,Image.PNG" default="required"  >
            <meta key="doc">
                Floating image
                    
                    Floating image filename (also called Source of moving)
            </meta>
        </arg>

        <arg name="inputTransformation"  physical="['TXT', 'MAT', 'NII', 'NII.GZ', 'NRRD']"   >
            <meta key="doc">
                Input trans.
                    
                    Input transformation
            </meta>
        </arg>

    </input>

    <output>
    
        <arg name="warpedImage"  physical="Image.NII,Image.NII.GZ,Image.NRRD,Image.PNG" default="warpedImage.nii"  >
            <meta key="doc">
                Warped image
                    
                    Warped floating image
            </meta>
        </arg>

        <arg name="warpedGrid"  physical="Image.NII,Image.NII.GZ,Image.NRRD,Image.PNG" default="warpedGrid.nii"  >
            <meta key="doc">
                Grid image
                    
                    Warped blank grid image
            </meta>
        </arg>

    </output>

    <parameters>
    
        <arg name="interpolation"  physical="vector.int" default="3"  >
            <meta key="doc">
                Interpolation order
                    
                    Interpolation order to use to warp the floating image
                    :Possible Values: ['0', '1', '3', '4']
            </meta>
        </arg>

        <arg name="paddingValue"  physical="float" default="0"  >
            <meta key="doc">
                Padding value
                    
                    Padding value
                    :Possible Values: []
            </meta>
        </arg>

    </parameters>
</msml:operator>